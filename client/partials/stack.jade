.stack
    h1 {{stack.title}}
    h3 {{stack.penalty}}

    p(ng-show="isCountingDown()") {{timeLeft}}
    button(ng-show="isInactive()", ng-click="checkIn()") Check In
    .members
        ul
            li(ng-repeat="member in members",
               ng-class="{inactive: isInactive(member.name)}") {{member.name}}
